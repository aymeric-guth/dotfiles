remove Mod2 = Num_Lock
clear Lock

! alt_l (keycode 64) -> super_l
remove Mod1 = Alt_L
! super_l (keycode 133) -> alt_l
remove Mod4 = Super_L
keycode 64 = Super_L
keycode 133 = Alt_L
add Mod4 = Super_L
add Mod1 = Alt_L

! caps lock (keycode 66) -> hyper
keycode 66 = Hyper_L
remove lock = Hyper_L
remove Mod4 = Hyper_L
add Mod3 = Hyper_L

! alt gr (keycode 108) -> super_r
remove Mod1 = Alt_R
keycode 108 = Super_R
add Mod4 = Super_R

! menu (keycode 135) -> compose (alt_r)
keycode 135 = Multi_key
