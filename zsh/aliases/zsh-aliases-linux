source $ZALIASES/zsh-aliases

alias ns='netstat -plntu'
alias ls='ls --color=auto'

# docker
alias up='docker-compose up --build'
alias down='docker-compose down'
alias run='docker-compose run'
#alias exe='docker exec -it'


alias nvim="XDG_CONFIG_HOME=${DOTCONF}/.config XDG_DATA_HOME=${DOTCONF}/.local/share PATH=$HOME/bin/lua-language-server:$PATH nvim"
alias python3="python3.7"
